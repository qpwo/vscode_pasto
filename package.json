{
  "name": "vscode-pasto",
  "displayName": "Pasto",
  "description": "Smart pasting commands: Paste Lines No Blanks, and Paste Pivot.",
  "version": "0.0.2",
  "icon": "icon.png",
  "publisher": "qpwo",
  "repository": {
    "type": "git",
    "url": "https://github.com/qpwo/vscode_pasto.git"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "pasto.pasteNewLines",
        "title": "Pasto: Paste Lines No Blanks"
      },
      {
        "command": "pasto.pastePivot",
        "title": "Pasto: Paste Pivot"
      }
    ],
    "keybindings": [
      {
        "command": "pasto.pasteNewLines",
        "key": "ctrl+shift+v",
        "mac": "ctrl+shift+v",
        "when": "editorTextFocus"
      },
      {
        "command": "pasto.pastePivot",
        "key": "ctrl+alt+v",
        "mac": "ctrl+cmd+v",
        "when": "editorTextFocus"
      }
    ]
  },
  "devDependencies": {
    "@vscode/vsce": "^3.6.0"
  }
}
