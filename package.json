{
  "name": "vscode-pasto",
  "displayName": "Pasto",
  "description": "paste one line, paste one word per line, and paste lines no blanks.",
  "version": "0.0.4",
  "icon": "icon.png",
  "publisher": "qpwo",
  "repository": {
    "type": "git",
    "url": "https://github.com/qpwo/vscode_pasto.git"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "pasto.pasteNewLines",
        "title": "Pasto: Paste Lines No Blanks"
      },
      {
        "command": "pasto.pasteNewLinesTerminal",
        "title": "Pasto: Paste Lines No Blanks (Terminal)"
      },
      {
        "command": "pasto.pasteOneLine",
        "title": "Pasto: Paste One Line"
      },
      {
        "command": "pasto.pasteOneLineTerminal",
        "title": "Pasto: Paste One Line (Terminal)"
      },
      {
        "command": "pasto.pasteWordsOnLines",
        "title": "Pasto: Paste One Word Per Line"
      },
      {
        "command": "pasto.pasteWordsOnLinesTerminal",
        "title": "Pasto: Paste One Word Per Line (Terminal)"
      }
    ],
    "keybindings": [
      {
        "command": "pasto.pasteNewLines",
        "key": "ctrl+shift+v",
        "mac": "ctrl+shift+v",
        "when": "editorTextFocus"
      },
      {
        "command": "pasto.pasteNewLinesTerminal",
        "key": "ctrl+shift+v",
        "mac": "ctrl+shift+v",
        "when": "terminalFocus"
      },
      {
        "command": "pasto.pasteOneLine",
        "key": "ctrl+alt+v",
        "mac": "ctrl+cmd+v",
        "when": "editorTextFocus"
      },
      {
        "command": "pasto.pasteOneLineTerminal",
        "key": "ctrl+alt+v",
        "mac": "ctrl+cmd+v",
        "when": "terminalFocus"
      },
      {
        "command": "pasto.pasteWordsOnLines",
        "key": "ctrl+alt+shift+v",
        "mac": "ctrl+cmd+shift+v",
        "when": "editorTextFocus"
      },
      {
        "command": "pasto.pasteWordsOnLinesTerminal",
        "key": "ctrl+alt+shift+v",
        "mac": "ctrl+cmd+shift+v",
        "when": "terminalFocus"
      }
    ]
  },
  "devDependencies": {
    "@vscode/vsce": "^3.6.0"
  }
}
